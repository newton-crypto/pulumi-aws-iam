{
    "name": "aws-iam",
    "types": {
        "aws-iam:index:PolicyStatement": {
            "type": "object",
            "properties": {
                "actions": {
                    "type": "array",
                    "items": {
                        "type" : "string"
                    }
                },
                "resources": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "effect": {
                    "type": "string"
                },
                "conditions": {
                    "type": "string"
                },
                "sid": {
                    "type": "string"
                }
            }
        }
    },
    "resources": {
        "aws-iam:index:Role": {
            "isComponent": true,
            "inputProperties": {
                "name": {
                    "type": "string",
                    "description": "Name of Role or Policy."
                },
                "env": {
                    "type": "string",
                    "description": "Environment used for role path."
                },
                "policy_statements": {
                    "type": "string",
                    "description": "Policy statements for access."
                },
                "assume_role_policy": {
                    "type": "string",
                    "description": "Policy that grants an entity permission to assume the role."
                },
                "managed_policy_arns": {
                    "type": "array",
                    "items":{
                        "$ref": "#/types/aws-iam:index:PolicyStatement"
                    },
                    "description": "ARNs of IAM managed policies."
                },
                "max_session_duration": {
                    "type": "integer",
                    "description": "Maximum session duration (in seconds) that you want to set for the specified role."
                },
                "name_is_prefix": {
                    "type": "boolean",
                    "description": ""
                }
            },
            "requiredInputs": [
                "name"
            ]
        },
        "aws-iam:index:Policy": {
            "isComponent": true,
            "inputProperties": {
                "name": {
                    "type": "string",
                    "description": "Name of Role or Policy."
                },
                "policy_statements": {
                    "type": "string",
                    "description": "Policy statements for access."
                },
                "description": {
                    "type": "string",
                    "description": "Description of Policy."
                },
                "name_suffix": {
                    "type": "string",
                    "description": "Optional suffix for Policy."
                }
            },
            "requiredInputs": [
                "name"
            ]
        }
    },
    "language": {
        "csharp": {
            "packageReferences": {
                "Pulumi": "3.*",
                "Pulumi.Aws": "5.*"
            }
        },
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/pulumi/pulumi-aws-iam/sdk/go/aws-iam"
        },
        "nodejs": {
            "dependencies": {
                "@pulumi/aws": "^5.0.0"
            },
            "devDependencies": {
                "typescript": "^3.7.0"
            }
        },
        "python": {
            "requires": {
                "pulumi": ">=3.0.0,<4.0.0",
                "pulumi-aws": ">=5.0.0,<6.0.0"
            }
        }
    }
}
